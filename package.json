{
  "name": "myapp",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prisma:migrate": "dotenv -e .env.prod -- pnpm prisma migrate deploy",
    "prisma:generate": "dotenv -e .env.prod -- pnpm prisma generate",
    "prisma:studio": "dotenv -e .env.prod -- pnpm prisma studio",
    "prisma:db-pull": "dotenv -e .env.prod -- pnpm prisma db pull",
    "prisma:db-push": "dotenv -e .env.prod -- pnpm prisma db push",
    "scrap:youtube_playlist": "dotenv -e .env.prod -- pnpm tsx .\\generate\\youtubePlaylist.ts"
  },
  "devDependencies": {
